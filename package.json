{
  "name": "flamingo-panel",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "deploy": "bun run build && wrangler deploy",
    "cf-typegen": "wrangler types",
    "db:generate": "drizzle-kit generate",
    "db:migrate:dev": "wrangler d1 migrations apply flamingo-db --local",
    "db:migrate:prod": "wrangler d1 migrations apply flamingo-db --remote",
    "db:studio": "LOCAL=1 drizzle-kit studio",
    "test": "vitest",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "check:ci": "biome check ."
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.3",
    "@cloudflare/vite-plugin": "^1.25.2",
    "@cloudflare/workers-types": "^4.20260219.0",
    "@tailwindcss/vite": "^4.2.0",
    "@tanstack/react-query": "^5.90.21",
    "@tanstack/react-router": "^1.161.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "better-sqlite3": "^12.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.31.9",
    "lucide-react": "^0.575.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwind-merge": "^3.5.0",
    "tailwindcss": "^4.2.0",
    "typescript": "^5.9.3",
    "ultracite": "^7.2.3",
    "vite": "^7.3.1",
    "wrangler": "^4.67.0"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.7.6",
    "@tanstack/react-start": "^1.161.3",
    "drizzle-orm": "^0.45.1",
    "hono": "^4.12.0",
    "jose": "^6.1.3",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.3",
    "sonner": "^2.0.7",
    "zod": "^4.3.6"
  }
}
