{
  "name": "flamingo-panel",
  "main": "src/web/server.ts",
  "compatibility_date": "2025-12-01",
  "compatibility_flags": ["nodejs_compat"],
  "vars": {
    "PANEL_URL": "https://urw-marks-growing-bacterial.trycloudflare.com",
    "OIDC_DISCOVERY_URL": "https://auth.tomascorreia.net/.well-known/openid-configuration"
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "flamingo-db",
      "database_id": "local"
    }
  ],
  "kv_namespaces": [
    {
      "binding": "KV",
      "id": "local"
    }
  ],
  "r2_buckets": [
    {
      "binding": "R2",
      "bucket_name": "flamingo-files"
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "name": "CONSOLE_SESSION",
        "class_name": "ConsoleSession"
      },
      {
        "name": "NODE_METRICS",
        "class_name": "NodeMetrics"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["ConsoleSession"]
    },
    {
      "tag": "v2",
      "new_classes": ["NodeMetrics"]
    }
  ]
}
